    <div class="events">
      <ul class="wrap">
        <% @week.each do |day, blocks| %>
          <li class="events-group">
            <div class="top-info"><span><%= day %></span></div>
            <ul>
              <% if blocks.size != 0%>
                <% blocks.each do |course_block| %>
                  <li class="single-event" data-start="<%= course_block.start_time %>" data-end="<%= course_block.end_time %>"  data-content="event-rowing-workout" data-event="event-1">
                    <a href="#0">
                      <em class="event-name"></em>
                    </a>
                  </li>
                <%end%>
              <%end%>
            </ul>
          </li>
        <%end%>
      </ul>
