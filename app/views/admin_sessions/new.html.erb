
<%= form_with(model: @session, local: true) do |form| %>

  <div class="form-group">
    <%= form.label :adminname, class:"text-light" %>
    <%= form.text_field :adminname, id: :adminname, class:"form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :password, class:"text-light" %>
    <%= form.password_field :password, id: :password, class:"form-control" %>
  </div>

  <div class="actions">
    <%= form.submit class:"btn btn-primary" %>
  </div>
<% end %>